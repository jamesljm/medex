// =============================================================================
// Booking Show AJAX
// =============================================================================
  

<% if current_user.type =="Doctor" %>
  $('#dashboard-content').empty().append('<%= j render "bookings/doctor_index" %>');

  $('.tab-pending').removeClass("active");

  $('.tab-confirm').addClass("active");

  $('.tab-past').removeClass("active");


<% else %>

  $('#dashboard-content').empty().append('<%= j render "bookings/patient_index" %>');

  $('.tab-pending').removeClass("active");

  $('.tab-confirm').addClass("active");

  $('.tab-past').removeClass("active");

<% end %>
