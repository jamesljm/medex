// =============================================================================
// Booking Show AJAX
// =============================================================================
  

<% if current_user.type =="Doctor" %>
  $('#dashboard-content').empty().removeClass('fade-in').addClass('fade-in').append('<%= j render "bookings/doctor_index" %>');

  $('.tab-pending').removeClass("active");

  $('.tab-confirm').addClass("active");

  $('.tab-past').removeClass("active");


<% else %>

  $('#dashboard-content').empty().removeClass('fade-in').addClass('fade-in').append('<%= j render "bookings/doctor_confirmed" %>');

<% end %>
