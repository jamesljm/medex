<% if current_user.type == 'Doctor' %>
    $('#dashboard-content').empty().removeClass('fade-in').addClass('fade-in').append('<%= j render "clinics/clinics" %>');
<% elsif current_user.type == 'Patient' %>
    $('#dashboard-content').empty().removeClass('fade-in').addClass('fade-in').append('<%= j render "clinics/index" %>');
<% end %>